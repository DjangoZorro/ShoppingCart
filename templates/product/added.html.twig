{% extends 'base.html.twig' %}

{% block title %}Hello!{% endblock %}

{% block body %}
<div class="example-wrapper">
    <h1>Added to your Shoppingcart!</h1>

    {% for array in cart %}
      <td> {{ array.name }} </td>
      <td> {{ array.price }} </td>
      <td> {{ array.quantity }} </td>
    {% else %}
      <tr>
        <td colspan="3">no records found</td>
      </tr>
    {% endfor %}
</div>
{% endblock %}
